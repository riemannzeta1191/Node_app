<table class="table table-inverse">
      <thead>
        <tr>
          <th>firstName</th>
          <th>lastName</th>
          <th>degree</th>
           <th>program</th>

        </tr>
      </thead>

      <tbody>
        <% for(var i=0; i < studentData.length; i++) { %>
       <tr>

         <td><%= studentData[i].firstName %></td>
         <td><%= studentData[i].lastName %></td>
         <td><%= studentData[i].degree %></td>
          <td><%= studentData[i].program %></td>


       </tr>
    <% } %>
      </tbody>

    </table>